<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml>
	<identity>
		<version number="0.0.1"/>
		<generation date="$Date$"/>
		<!-- name.en(yat)="Yambeta" -->
		<language type="yat"/>
		<special xmlns:sil="urn://www.sil.org/ldml/0.1">
			<sil:identity defaultRegion="CM"/>
		</special>
	</identity>
	<characters>
		<exemplarCharacters>[a á b c d e é ɛ {ɛ́} ə {ə́} f g i í j k l m n {ny} ŋ o ó ɔ {ɔ́} p s t u ú w y ʼ]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[h q r v x z]</exemplarCharacters>
		<exemplarCharacters type="index">[A B C D E Ɛ Ə F G I J K L M N {Ny} Ŋ O Ɔ P S T U W Y ʼ]</exemplarCharacters>
		<exemplarCharacters type="punctuation">[, . ; ' "! ?]</exemplarCharacters>
	</characters>
	<!--  Enter the plural rules (template example is for only one plural form)  -->
	<plurals>
		<pluralRule count="one">n is 1</pluralRule>
	</plurals>
</ldml>
