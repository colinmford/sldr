<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="0.0.1"/>
		<generation date="$Date$"/>
		<!-- name.en(yas)="Nugunu (Cameroon)" -->
		<language type="yas"/>
		<special>
			<sil:identity defaultRegion="CM"/>
		</special>
	</identity>
	<characters>
		<exemplarCharacters>[a á b c d e é ɛ {ɛ́} f g h i í k l m n {nc} {nd} {nt} {ny} ŋ {ŋk} {ŋg} o ó ɔ {ɔ́} p s t u ú y ]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[j q r v w x]</exemplarCharacters>
		<exemplarCharacters type="index">[A B C D E Ɛ F G H I K L M N {Nc} {Nd} {Nt} {Ny} Ŋ {Ŋk} {Ŋg} O Ɔ P S T U Y ]</exemplarCharacters>
		<exemplarCharacters type="punctuation">[, . ; ' " ! ?]</exemplarCharacters>
	</characters>
	<!--  Enter the plural rules (template example is for only one plural form)  -->
	<plurals>
		<pluralRule count="one">n is 1</pluralRule>
	</plurals>
</ldml>
