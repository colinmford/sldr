<?xml version="1.0" encoding="utf-8"?>
<!-- Please enter language data in the fields below.  All data should be entered in English -->
<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="0.0.1"/>
		<!-- name.en(kdh)="Tem" -->
		<language type="kdh"/>
		<special>
			<sil:identity defaultRegion="TG" script="Arab"/>
		</special>
	</identity>
	<localeDisplayNames>
		<special>
			<sil:names>
				<sil:name xml:lang="en">Tem</sil:name>
			</sil:names>
		</special>
	</localeDisplayNames>
	<layout>
		<!--  Enter the orientation attributes  -->
		<orientation>
			<characterOrder>right-to-left</characterOrder>
		</orientation>
	</layout>
	<characters>
		<exemplarCharacters>[a b c d e f g h i j k l m n o p r s t u v w y ÿ z ŋ ɔ ɖ ɛ ɩ ʊ \u0301]</exemplarCharacters>
		<exemplarCharacters type="auxiliary">[\u200C\u200D\u200E\u200F]</exemplarCharacters>
		<exemplarCharacters type="punctuation" draft="generated">[! " ' ( ) , \- . \: ; ? \[ \] « » ‑]</exemplarCharacters>
		<exemplarCharacters type="index" draft="generated">[A B C D E F G H I J K L M N O P R S T U V W Y Ÿ Z Ŋ Ɔ Ɖ Ɛ Ɩ Ʊ]</exemplarCharacters>
	</characters>
	<delimiters>
		<quotationStart>“</quotationStart>
		<quotationEnd>”</quotationEnd>
		<alternateQuotationStart>‘</alternateQuotationStart>
		<alternateQuotationEnd>’</alternateQuotationEnd>
		<special>
			<sil:matched-pairs/>
			<sil:quotation-marks>
				<sil:quotation open="“" close="”" level="3"/>
			</sil:quotation-marks>
		</special>
	</delimiters>
	<special>
		<sil:external-resources>
			<sil:font name="Charis SIL" types="default" size="0.85" draft="generated"/>
			<sil:font name="Noto Sans Arabic" types="ui">
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSansArabic/NotoSansArabic-Regular.ttf</sil:url>
			</sil:font>
			<sil:font name="Scheherazade" types="default">
				<sil:url>https://wirl.api.sil.org/Scheherazade&amp;type=ttf</sil:url>
			</sil:font>
			<sil:kbd id="tem_kdh" type="kmp">
				<sil:url draft="generated">https://keyman.com/go/keyboard/tem_kdh/download/kmp</sil:url>
			</sil:kbd>
			<sil:sampletext type="udhr">
				<sil:url>https://unicode.org/udhr/d/udhr_kdh.txt</sil:url>
			</sil:sampletext>
		</sil:external-resources>
	</special>
	<special>
		<sil:note><![CDATA[
			# Issues
			1. currently kdh = kdh-Arab and data in this file is Latn script.
		]]></sil:note>
	</special>
</ldml>
