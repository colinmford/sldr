<ldml xmlns:sil="urn://www.sil.org/ldml/0.1">
	<identity>
		<version number="$Revision$"/>
		<language type="lrc"/>
		<territory type="IQ"/>
		<special>
			<sil:identity source="cldr" defaultRegion="IQ" script="Arab"/>
		</special>
	</identity>
	<dates>
		<calendars>
			<calendar type="gregorian">
				<timeFormats>
					<timeFormatLength type="full">
						<timeFormat>
							<pattern>h:mm:ss a zzzz</pattern>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="long">
						<timeFormat>
							<pattern>h:mm:ss a z</pattern>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="medium">
						<timeFormat>
							<pattern>h:mm:ss a</pattern>
						</timeFormat>
					</timeFormatLength>
					<timeFormatLength type="short">
						<timeFormat>
							<pattern>h:mm a</pattern>
						</timeFormat>
					</timeFormatLength>
				</timeFormats>
			</calendar>
		</calendars>
	</dates>
	<special>
		<sil:external-resources>
			<sil:font name="Noto Sans Arabic" types="ui">
				<sil:url>https://github.com/googlefonts/noto-fonts/raw/main/hinted/ttf/NotoSansArabic/NotoSansArabic-Regular.ttf</sil:url>
			</sil:font>
			<sil:font name="Scheherazade">
				<sil:url>https://wirl.api.sil.org/Scheherazade&amp;type=ttf</sil:url>
			</sil:font>
		</sil:external-resources>
	</special>
</ldml>
